{
    "asciidocExt": {},
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
        "parser": {
            "wrapStandAloneImageWithinParagraph": true
        },
        "renderer": {
            "unsafe": true,
            "xhtml": true
        }
    },
    "highlight": {
        "codeFences": true,
        "anchorLineNos": true,
        "lineNoStart": 1,
        "lineNos": true,
        "guessSyntax": false,
        "lineNumbersInTable": false,
        "style": "doom-one2",
        "noClasses":true,
        "noHl": false,
        "hl_Lines": "",
        "hl_inline": false,
        "lineAnchors": "abc",
        "tabWidth": 4
        
    },
    "tableOfContents": {
        "startLevel": 1,
        "endLevel": 3,
        "ordered": true
    }
}